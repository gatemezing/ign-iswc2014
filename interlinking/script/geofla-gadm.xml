<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
	<PREFIX>
		<NAMESPACE>http://data.ign.fr/def/geofla#</NAMESPACE>
		<LABEL>geofla</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://data.ign.fr/id/geofla/</NAMESPACE>
		<LABEL>geoign</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
		<LABEL>rdf</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
		<LABEL>rdfs</LABEL>
	</PREFIX>
	<PREFIX>
		
	<PREFIX>
		<NAMESPACE>http://gadm.geovocab.org/ontology#</NAMESPACE>
		<LABEL>gadm</LABEL>
	</PREFIX>
	
	<PREFIX>
		<NAMESPACE>http://geovocab.org/spatial#</NAMESPACE>
		<LABEL>spatial</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
		<LABEL>skos</LABEL>
	</PREFIX>
	<SOURCE>
		<ID>geofla</ID>
		<ENDPOINT>http://eventmedia.eurecom.fr/sparql</ENDPOINT>
		<VAR>?x</VAR>
		<PAGESIZE>5000</PAGESIZE>
		<RESTRICTION>?x a geofla:Commune</RESTRICTION>
		<PROPERTY>rdfs:label AS nolang->lowercase RENAME name</PROPERTY>
		<TYPE>SPARQL</TYPE>
	</SOURCE>
	<TARGET>
		<ID>gadm</ID>
		<ENDPOINT>C:\LIMES\Mappings\datasets\gadm-dataset.nt</ENDPOINT>
		<VAR>?y</VAR>
		<PAGESIZE>5000</PAGESIZE>
		<RESTRICTION>?y rdf:type spatial:Feature; gadm:in_country "France"</RESTRICTION>
		<PROPERTY>rdfs:label AS nolang->lowercase RENAME name</PROPERTY>
		<TYPE>N-TRIPLE</TYPE>
	</TARGET>
	<METRIC>trigrams(x.name, y.name)</METRIC>
	<ACCEPTANCE>
		<THRESHOLD>0.98</THRESHOLD>
		<FILE>geofla-gadm-matched.nt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</ACCEPTANCE>
	<REVIEW>
		<THRESHOLD>0.95</THRESHOLD>
		<FILE>geofla-gadm-review.nt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</REVIEW>
	<EXECUTION>Simple</EXECUTION>
	<OUTPUT>N3</OUTPUT>
</LIMES>
