<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
	<PREFIX>
		<NAMESPACE>http://data.ign.fr/def/geofla#</NAMESPACE>
		<LABEL>geofla</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://fr.dbpedia.org/resource/</NAMESPACE>
		<LABEL>dbpedia-fr</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://data.ign.fr/id/geofla/</NAMESPACE>
		<LABEL>geoign</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
		<LABEL>foaf</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
		<LABEL>rdf</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
		<LABEL>rdfs</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
		<LABEL>dbpedia-owl</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://fr.dbpedia.org/property/</NAMESPACE>
		<LABEL>prop-fr</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
		<LABEL>skos</LABEL>
	</PREFIX>
	<SOURCE>
		<ID>geofla</ID>
		<ENDPOINT>http://eventmedia.eurecom.fr/sparql</ENDPOINT>
		<VAR>?x</VAR>
		<PAGESIZE>5000</PAGESIZE>
		<RESTRICTION>?x a geofla:Region</RESTRICTION>
		<PROPERTY>geofla:numInsee RENAME codeinsee</PROPERTY>
		<PROPERTY>rdfs:label AS nolang->lowercase RENAME name</PROPERTY>
		<TYPE>SPARQL</TYPE>
	</SOURCE>
	<TARGET>
		<ID>dbpedia-fr</ID>
		<ENDPOINT>http://fr.dbpedia.org/sparql</ENDPOINT>
		<VAR>?y</VAR>
		<PAGESIZE>5000</PAGESIZE>
		<RESTRICTION>{{?y rdf:type dbpedia-owl:Place} UNION {?y rdf:type skos:Concept}}</RESTRICTION>
		<PROPERTY>prop-fr:insee RENAME code</PROPERTY>
		<PROPERTY>rdfs:label AS nolang->lowercase RENAME name</PROPERTY>
		<TYPE>SPARQL</TYPE>
	</TARGET>
	<METRIC>ADD(0.3*levenstein(x.name, y.name), 0.7*trigrams(x.codeinsee, y.code))</METRIC>
	<ACCEPTANCE>
		<THRESHOLD>0.98</THRESHOLD>
		<FILE>geoflaRegions-dbpedia-matched.nt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</ACCEPTANCE>
	<REVIEW>
		<THRESHOLD>0.90</THRESHOLD>
		<FILE>geoflaRegions-dbpedia-review.nt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</REVIEW>
	<EXECUTION>Simple</EXECUTION>
	<OUTPUT>N3</OUTPUT>
</LIMES>
