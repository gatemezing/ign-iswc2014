<rdf:RDF xmlns:dcterms="http://purl.org/dc/terms/" xmlns:cc="http://creativecommons.org/ns#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:ignf="http://data.ign.fr/def/ignf#" xmlns:geom="http://data.ign.fr/def/geometrie#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:vann="http://purl.org/vocab/vann/" xmlns:ngeo="http://geovocab.org/geometry#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:sf="http://www.opengis.net/ont/sf#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
	<owl:Ontology rdf:about="http://data.ign.fr/def/geometrie">
		<dcterms:description xml:lang="fr">Ontologie des primitives géométriques représentant la forme et la localisation d'entités topographiques.</dcterms:description>
		<dcterms:title xml:lang="fr">Ontologie des primitives géométriques</dcterms:title>
		<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/" />
