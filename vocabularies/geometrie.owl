	<owl:Class rdf:about="http://data.ign.fr/def/geometrie#Line">
		<rdfs:comment xml:lang="fr">Primitive géométrique spécialisant la primitive Polyligne et composée de seulement deux points.</rdfs:comment>
		<rdfs:label xml:lang="fr">Ligne</rdfs:label>
		<rdfs:label xml:lang="en">Line</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://data.ign.fr/def/geometrie#LineString" />
		<owl:equivalentClass>
			<owl:Restriction>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf>
							<rdf:Description>
								<rdf:first rdf:resource="http://data.ign.fr/def/geometrie#PointsList" />
								<rdf:rest>
									<rdf:Description>
										<rdf:first>
											<owl:Restriction>
												<owl:someValuesFrom>
													<owl:Class>
														<owl:intersectionOf>
															<rdf:Description>
																<rdf:first rdf:resource="http://data.ign.fr/def/geometrie#PointsList" />
																<rdf:rest>
																	<rdf:Description>
																		<rdf:first>
																			<owl:Restriction>
																				<owl:onProperty rdf:resource="http://data.ign.fr/def/geometrie#rest" />
																				<owl:hasValue rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil" />
																			</owl:Restriction>
																		</rdf:first>
																		<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil" />
																	</rdf:Description>
																</rdf:rest>
															</rdf:Description>
														</owl:intersectionOf>
													</owl:Class>
												</owl:someValuesFrom>
												<owl:onProperty>
													<owl:ObjectProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#rest">
														<rdfs:comment xml:lang="fr">La suite d'une liste.</rdfs:comment>
														<rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List" />
														<rdfs:label xml:lang="en">rest</rdfs:label>
														<rdfs:label xml:lang="fr">rest</rdfs:label>
														<rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List" />
													</owl:ObjectProperty>
												</owl:onProperty>
											</owl:Restriction>
										</rdf:first>
										<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil" />
									</rdf:Description>
								</rdf:rest>
							</rdf:Description>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
				<owl:onProperty rdf:resource="http://data.ign.fr/def/geometrie#points" />
			</owl:Restriction>
		</owl:equivalentClass>
		<rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/sf#Line" />
	</owl:Class>
	<owl:Class rdf:about="http://data.ign.fr/def/geometrie#GeometryCollection">
