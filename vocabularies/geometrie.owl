		</dcterms:contributor>
		<dcterms:contributor>
			<foaf:Person rdf:about="http://recherche.ign.fr/labos/cogit/cv.php?prenom=Bénédicte&amp;nom=Bucher" />
		</dcterms:contributor>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-06-11</dcterms:issued>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-10-01</dcterms:modified>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-28</dcterms:modified>
		<dcterms:publisher rdf:resource="http://fr.dbpedia.org/resource/Institut_national_de_l%27information_g%C3%A9ographique_et_foresti%C3%A8re" />
		<vann:preferredNamespacePrefix>geom</vann:preferredNamespacePrefix>
		<vann:preferredNamespaceUri rdf:resource="http://data.ign.fr/def/geometrie#" />
		<dc:rights>Copyright © 2013 IGN</dc:rights>
