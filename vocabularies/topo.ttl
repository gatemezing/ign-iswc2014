	rdfs:range [ a owl:DataRange;  owl:oneOf ("63" "90" "150" "225" "400" "NR") ].

topo:posSol a owl:DatatypeProperty;
	rdfs:comment "Position par rapport au sol. Donne le niveau de l’objet par rapport à la surface du sol (valeur nulle pour un objet au sol, négative pour un objet situé au-dessous du niveau du sol et positive pour un objet en sursol)."@fr;
