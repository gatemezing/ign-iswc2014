# ---- Les propriétes -----------------------------------------------------------------------------------------

# ---- La propriété pour relier les entités au(x) concept(s) SKOS qui précise(nt) leur nature -----------------

topo:nature a owl:ObjectProperty;
	rdfs:label "nature"@fr;
	rdfs:label "nature"@en;	
	rdfs:comment "Relie une entité topographique au concept qui précise sa nature."@fr;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:domain topo:EntiteTopographique;
	rdfs:range skos:Concept.

topo:typeDeZAI a owl:ObjectProperty;
	rdfs:label "type de ZAI"@fr;
	rdfs:label "type of Area Of Interest (AOI)"@en;
	rdfs:subPropertyOf topo:nature; 
	rdfs:comment "Relie une instance de zone d'activité et d'intérêt à une instance de skos:Concept définie dans la liste 'zai' qui précise sa nature."@fr;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:domain topo:ZoneActiviteInteret;
	rdfs:range [ a owl:Restriction;
					owl:onProperty skos:inScheme;
					owl:hasValue <http://data.ign.fr/id/codes/topo/typedezai/liste> ].

topo:typeDeBatiment a owl:ObjectProperty;
	rdfs:label "type de bâtiment"@fr;
	rdfs:label "type of building "@en;
	rdfs:subPropertyOf topo:nature;
	rdfs:comment "Relie une instance de bâtiment à une instance de skos:Concept définie dans la liste 'bati' qui précise sa nature."@fr;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:domain topo:Batiment;
	rdfs:range [ a owl:Restriction;
					owl:onProperty skos:inScheme;
					owl:hasValue <http://data.ign.fr/id/codes/topo/typedebatiment/liste> ].

topo:typeDeConstruction a owl:ObjectProperty;
	rdfs:label "type de construction"@fr;
	rdfs:label "type of structure"@en;
	rdfs:subPropertyOf topo:nature; 
	rdfs:comment "Relie une instance de construction à une instance de skos:Concept définie dans la liste 'cnstr' qui précise sa nature."@fr;
